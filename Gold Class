 class Gold : Item
                {
                    public override bool IsValidMove(int xpos, int ypos)
                    {
                        throw new NotImplementedException();
                    }

                    private int GoldDrop;

                    public int golddrop 
                    {
                        get { return GoldDrop; }
                        set { GoldDrop = value; }
                    }


                }

                private int Random()
                {
                    Random random = new Random();
                    int randomNumber = random.Next(1,6);
                    return randomNumber;
                }
